    SET ESTADOS_ID_ESTADO_FK = P_ESTADOS_ID_ESTADO_FK
    WHERE METODOS_PAGO_ID_PAGO_PK = P_METODOS_PAGO_ID_PAGO_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

END FIDE_PROYECTO_FINAL_PKG;

/
--------------------------------------------------------
--  DDL for Function FIDE_PEDIDOS_PENDIENTES_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "PROYECTOLENGUAJES"."FIDE_PEDIDOS_PENDIENTES_FN" (pIdUsuario IN NUMBER)
RETURN NUMBER
AS
    vPedidoId NUMBER;
BEGIN
    SELECT PEDIDOS_ID_PEDIDO_PK
    INTO vPedidoId
    FROM FIDE_PEDIDOS_TB
    WHERE USUARIOS_ID_USUARIO_FK = pIdUsuario
      AND ESTADOS_ID_ESTADO_FK = 1;

    RETURN vPedidoId;
EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN NULL;
END FIDE_PEDIDOS_PENDIENTES_FN;

/
--------------------------------------------------------
--  DDL for Function FIDE_TRAER_TIPO_USUARIO_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "PROYECTOLENGUAJES"."FIDE_TRAER_TIPO_USUARIO_FN" (
    p_correo IN VARCHAR2,
    p_contrasena IN VARCHAR2
) RETURN NUMBER
IS
    v_tipo_usuario NUMBER;
BEGIN
    SELECT TIPO_USUARIO_ID_FK
    INTO v_tipo_usuario
    FROM FIDE_USUARIOS_TB
    WHERE EMAIL = p_correo
      AND CONTRASENA = p_contrasena
      AND ESTADOS_ID_ESTADO_FK = 1; -- Opcional: solo usuarios activos

    RETURN v_tipo_usuario;

EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN NULL; -- Usuario no encontrado o contraseña incorrecta
    WHEN OTHERS THEN
        RETURN NULL; -- Cualquier otro error
END FIDE_TRAER_TIPO_USUARIO_FN;

/
--------------------------------------------------------
--  DDL for Function FIDE_VALIDAR_LOGIN_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "PROYECTOLENGUAJES"."FIDE_VALIDAR_LOGIN_FN" (
    P_CORREO       IN VARCHAR2,
    P_CONTRASENA   IN VARCHAR2
) RETURN NUMBER
IS
    v_idUsuario NUMBER;
BEGIN
    SELECT USUARIO_ID_USUARIO_PK
    INTO v_idUsuario
    FROM FIDE_USUARIOS_TB
    WHERE EMAIL = P_CORREO
      AND CONTRASENA = P_CONTRASENA;

    RETURN v_idUsuario;

EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN 0; 
END FIDE_VALIDAR_LOGIN_FN;

