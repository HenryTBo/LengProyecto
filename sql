--------------------------------------------------------
--  DDL for View FIDE_ALMACENAMIENTO_LISTAR_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_ALMACENAMIENTO_LISTAR_V" ("ALMACENADO_ID_ALMACENADO_PK", "DESCRIPCION_ALMACENADO") AS 
  SELECT 
   A.ALMACENADO_ID_ALMACENADO_PK,
   A.DESCRIPCION_ALMACENADO
FROM FIDE_ALMACENADO_TB A
WHERE ESTADOS_ID_ESTADO_FK = 1
;
--------------------------------------------------------
--  DDL for View FIDE_CARRITO_DETALLE_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_CARRITO_DETALLE_V" ("IDCARRITO", "IDUSUARIO", "IDPRODUCTO", "NOMBREPRODUCTO", "IMAGEN", "DESCRIPCION", "CANTIDADCARRITO", "PRECIOPRODUCTO", "FECHA", "SUBTOTAL", "IMPUESTO", "TOTAL") AS 
  SELECT
    c.CARRITO_ID_CARRITO_PK AS idCarrito,
    c.USUARIOS_ID_USUARIO_FK AS idUsuario,
    c.PRODUCTOS_ID_PRODUCTO_FK AS idProducto,
    p.NOMBRE AS nombreProducto,
    p.IMAGEN,
    p.DESCRIPCION,
    c.CANTIDAD AS cantidadCarrito,
    p.PRECIO_UNITARIO AS precioProducto,
    c.FECHA_AGREGADO AS fecha,
    (c.CANTIDAD * p.PRECIO_UNITARIO) AS subtotal,
    (c.CANTIDAD * p.PRECIO_UNITARIO * 0.13) AS impuesto,
    (c.CANTIDAD * p.PRECIO_UNITARIO * 1.13) AS total
FROM FIDE_CARRITO_TB c
JOIN FIDE_PRODUCTOS_TB p
    ON c.PRODUCTOS_ID_PRODUCTO_FK = p.PRODUCTO_ID_PRODUCTO_PK
WHERE c.ESTADOS_ID_ESTADO_FK = 1
;
--------------------------------------------------------
--  DDL for View FIDE_CATEGORIAS_LISTAR_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_CATEGORIAS_LISTAR_V" ("CATEGORIA_ID_CATEGORIA_PK", "NOMBRE_CATEGORIA") AS 
  SELECT CATEGORIA_ID_CATEGORIA_PK, NOMBRE_CATEGORIA
FROM FIDE_CATEGORIA_TB
WHERE ESTADOS_ID_ESTADO_FK = 1
;
--------------------------------------------------------
--  DDL for View FIDE_CONTACTOS_ADMIN_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_CONTACTOS_ADMIN_V" ("CONTACTO_ID_CONTACTO_PK", "USUARIOS_ID_USUARIO_FK", "ASUNTO", "MENSAJE", "FECHA_ENVIO", "ESTADO") AS 
  SELECT
    c.CONTACTO_ID_CONTACTO_PK,
    c.USUARIOS_ID_USUARIO_FK,
    c.ASUNTO,
    c.MENSAJE,
    c.FECHA_ENVIO,
    e.NOMBRE_ESTADO AS ESTADO
FROM
    FIDE_CONTACTO_TB c
LEFT JOIN
    FIDE_ESTADOS_TB e
ON
    c.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View FIDE_DETALLE_PEDIDO_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_DETALLE_PEDIDO_V" ("ID_PEDIDO", "ESTADO_DETALLE_ID", "ESTADOS", "ESTADO_DETALLE_NOMBRE", "ID_PRODUCTO", "NOMBRE_PRODUCTO", "CANTIDAD", "PRECIO", "SUBTOTAL") AS 
  SELECT  dp.PEDIDOS_ID_PEDIDO_FK AS ID_PEDIDO,
        dp.ESTADOS_ID_ESTADO_FK AS ESTADO_DETALLE_ID,
        e.ESTADOS_ID_ESTADO_PK AS ESTADOS,
        e.NOMBRE_ESTADO AS ESTADO_DETALLE_NOMBRE,
        p.PRODUCTO_ID_PRODUCTO_PK AS ID_PRODUCTO,
        p.NOMBRE AS NOMBRE_PRODUCTO,
        dp.CANTIDAD AS CANTIDAD,
        dp.PRECIO_UNITARIO AS PRECIO,
        (dp.CANTIDAD * dp.PRECIO_UNITARIO) AS SUBTOTAL
FROM    FIDE_DETALLE_PEDIDO_TB dp
JOIN    FIDE_PRODUCTOS_TB p
        ON dp.PRODUCTOS_ID_PRODUCTO_FK = p.PRODUCTO_ID_PRODUCTO_PK
JOIN    FIDE_ESTADOS_TB e
        ON dp.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View FIDE_FACTURACION_LISTA_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_FACTURACION_LISTA_V" ("FACTURACION_ID_FACTURA_PK", "PEDIDOS_ID_PEDIDO_FK", "METODOS_PAGO_ID_PAGO_FK", "METODO_PAGO", "ESTADOS_ID_ESTADO_FK", "ESTADO", "FECHA_EMISION", "DESCUENTOS", "SUBTOTAL", "IVA", "TOTAL_FACTURADO") AS 
  SELECT
    f.FACTURACION_ID_FACTURA_PK,
    f.PEDIDOS_ID_PEDIDO_FK,
    f.METODOS_PAGO_ID_PAGO_FK,
    mp.NOMBRE_METODO AS METODO_PAGO,
    f.ESTADOS_ID_ESTADO_FK,
    e.NOMBRE_ESTADO AS ESTADO,
    f.FECHA_EMISION,
    f.DESCUENTOS,
    f.SUBTOTAL,
    f.IVA,
    f.TOTAL_FACTURADO
FROM FIDE_FACTURACION_TB f
LEFT JOIN FIDE_METODOS_PAGO_TB mp
    ON f.METODOS_PAGO_ID_PAGO_FK = mp.METODOS_PAGO_ID_PAGO_PK
LEFT JOIN FIDE_ESTADOS_TB e
    ON f.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View FIDE_LISTAR_ESTADOS_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_LISTAR_ESTADOS_V" ("ID_ESTADO", "ESTADO") AS 
  SELECT  e.ESTADOS_ID_ESTADO_PK AS ID_ESTADO,
e.NOMBRE_ESTADO AS ESTADO
FROM    FIDE_ESTADOS_TB e
;
--------------------------------------------------------
--  DDL for View FIDE_LISTAR_PRODUCTOS_ADMIN_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_LISTAR_PRODUCTOS_ADMIN_V" ("ID_PRODUCTO", "NOMBRE_PRODUCTO", "DESCRIPCION_PRODUCTO", "PRECIO_UNITARIO", "IMAGEN", "ID_CATEGORIA", "NOMBRE_CATEGORIA", "ID_PRESENTACION", "TIPO_PRESENTACION", "DESCRIPCION_PRESENTACION", "ID_ESTADO", "ESTADO") AS 
  SELECT
    p.PRODUCTO_ID_PRODUCTO_PK      AS ID_PRODUCTO,
    p.NOMBRE                       AS NOMBRE_PRODUCTO,
    p.DESCRIPCION                  AS DESCRIPCION_PRODUCTO,
    p.PRECIO_UNITARIO,
    p.IMAGEN,
    c.CATEGORIA_ID_CATEGORIA_PK    AS ID_CATEGORIA,
    c.NOMBRE_CATEGORIA,
    pr.PRESENTACIONES_ID_PRESENTACION_PK AS ID_PRESENTACION,
    pr.TIPO_PRESENTACION,
    pr.DESCRIPCION                  AS DESCRIPCION_PRESENTACION,
    e.ESTADOS_ID_ESTADO_PK          AS ID_ESTADO,
    e.NOMBRE_ESTADO                 AS ESTADO
FROM
    FIDE_PRODUCTOS_TB p
INNER JOIN FIDE_CATEGORIA_TB c
    ON p.CATEGORIA_ID_CATEGORIA_FK = c.CATEGORIA_ID_CATEGORIA_PK
INNER JOIN FIDE_PRESENTACIONES_TB pr
    ON p.PRESENTACIONES_ID_PRESENTACION_FK = pr.PRESENTACIONES_ID_PRESENTACION_PK
INNER JOIN FIDE_ESTADOS_TB e
    ON p.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View FIDE_LISTAR_PRODUCTOS_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_LISTAR_PRODUCTOS_V" ("ID_PRODUCTO", "NOMBRE_PRODUCTO", "DESCRIPCION_PRODUCTO", "PRECIO_UNITARIO", "IMAGEN", "NOMBRE_CATEGORIA", "TIPO_PRESENTACION", "DESCRIPCION_PRESENTACION") AS 
  SELECT
    p.PRODUCTO_ID_PRODUCTO_PK      AS ID_PRODUCTO,
    p.NOMBRE                       AS NOMBRE_PRODUCTO,
    p.DESCRIPCION                   AS DESCRIPCION_PRODUCTO,
    p.PRECIO_UNITARIO,
    p.IMAGEN,
    c.NOMBRE_CATEGORIA,
    pr.TIPO_PRESENTACION,
    pr.DESCRIPCION                  AS DESCRIPCION_PRESENTACION
FROM
    FIDE_PRODUCTOS_TB p
INNER JOIN fide_categoria_tb c
    ON p.CATEGORIA_ID_CATEGORIA_FK = c.CATEGORIA_ID_CATEGORIA_PK
INNER JOIN FIDE_PRESENTACIONES_TB pr
    ON p.PRESENTACIONES_ID_PRESENTACION_FK = pr.PRESENTACIONES_ID_PRESENTACION_PK
WHERE
    p.ESTADOS_ID_ESTADO_FK = 1
;
--------------------------------------------------------
--  DDL for View FIDE_METODOS_PAGO_LISTAR_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_METODOS_PAGO_LISTAR_V" ("METODOS_PAGO_ID_PAGO_PK", "NOMBRE_METODO", "DESCRIPCION", "ESTADOS_ID_ESTADO_FK", "FECHA_CREACION", "FECHA_MODIFICACION", "CREADO_POR", "MODIFICADO_POR", "ACCION") AS 
  SELECT 
    METODOS_PAGO_ID_PAGO_PK,
    NOMBRE_METODO,
    DESCRIPCION,
    ESTADOS_ID_ESTADO_FK,
    FECHA_CREACION,
    FECHA_MODIFICACION,
    CREADO_POR,
    MODIFICADO_POR,
    ACCION
FROM FIDE_METODOS_PAGO_TB
;
--------------------------------------------------------
--  DDL for View FIDE_PEDIDOS_LISTAR_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_PEDIDOS_LISTAR_V" ("IDPEDIDO", "IDUSUARIO", "NOMBREUSUARIO", "IDESTADO", "NOMBREESTADO", "FECHAPEDIDO", "TOTAL") AS 
  SELECT 
    P.PEDIDOS_ID_PEDIDO_PK   AS IDPEDIDO,
    P.USUARIOS_ID_USUARIO_FK AS IDUSUARIO,
    U.NOMBRE                 AS NOMBREUSUARIO,
    P.ESTADOS_ID_ESTADO_FK   AS IDESTADO,
    E.NOMBRE_ESTADO           AS NOMBREESTADO,
   TO_CHAR(FECHA_PEDIDO, 'DD/MM/YYYY') AS FECHAPEDIDO,
    P.TOTAL                  AS TOTAL
FROM 
    FIDE_PEDIDOS_TB P
    INNER JOIN FIDE_USUARIOS_TB U ON P.USUARIOS_ID_USUARIO_FK = U.USUARIO_ID_USUARIO_PK
    INNER JOIN FIDE_ESTADOS_TB E  ON P.ESTADOS_ID_ESTADO_FK = E.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View FIDE_PEDIDOS_PENDIENTES_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_PEDIDOS_PENDIENTES_V" ("PEDIDOS_ID_PEDIDO_PK", "USUARIOS_ID_USUARIO_FK", "TOTAL", "ESTADOS_ID_ESTADO_FK", "PRODUCTOS_ID_PRODUCTO_FK", "CANTIDAD", "NOMBRE", "PRECIO_UNITARIO") AS 
  SELECT 
    p.PEDIDOS_ID_PEDIDO_PK,
    p.USUARIOS_ID_USUARIO_FK,
    p.TOTAL,
    p.ESTADOS_ID_ESTADO_FK,
    pd.PRODUCTOS_ID_PRODUCTO_FK,
    pd.CANTIDAD,
    pr.NOMBRE,
    pr.PRECIO_UNITARIO
FROM FIDE_PEDIDOS_TB p
JOIN FIDE_DETALLE_PEDIDO_TB pd
    ON p.PEDIDOS_ID_PEDIDO_PK = pd.PEDIDOS_ID_PEDIDO_FK
JOIN FIDE_PRODUCTOS_TB pr
    ON pd.PRODUCTOS_ID_PRODUCTO_FK = pr.PRODUCTO_ID_PRODUCTO_PK
WHERE p.ESTADOS_ID_ESTADO_FK = 3
;
--------------------------------------------------------
--  DDL for View FIDE_PRESENTACIONES_LISTAR_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_PRESENTACIONES_LISTAR_V" ("PRESENTACIONES_ID_PRESENTACION_PK", "TIPO_PRESENTACION") AS 
  SELECT PRESENTACIONES_ID_PRESENTACION_PK, TIPO_PRESENTACION
FROM FIDE_PRESENTACIONES_TB
WHERE ESTADOS_ID_ESTADO_FK = 1
;
--------------------------------------------------------
--  DDL for View FIDE_PRODUCTOS_SIN_INVENTARIO_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_PRODUCTOS_SIN_INVENTARIO_V" ("ID_PRODUCTO", "NOMBRE_PRODUCTO") AS 
  SELECT 
    P.PRODUCTO_ID_PRODUCTO_PK AS ID_PRODUCTO,
    P.NOMBRE AS NOMBRE_PRODUCTO
FROM FIDE_PRODUCTOS_TB P
WHERE P.PRODUCTO_ID_PRODUCTO_PK NOT IN (
    SELECT I.PRODUCTOS_ID_PRODUCTO_FK
    FROM FIDE_INVENTARIO_TB I
)
;
--------------------------------------------------------
--  DDL for View FIDE_USUARIOS_ADMIN_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_USUARIOS_ADMIN_V" ("USUARIO_ID_USUARIO_PK", "NOMBRE", "TELEFONO", "EMAIL", "NOMBRE_TIPO_USUARIO", "NOMBRE_CARGO", "ESTADO") AS 
  SELECT
    u.USUARIO_ID_USUARIO_PK,
    u.NOMBRE,
    u.TELEFONO,
    u.EMAIL,
    tu.NOMBRE_TIPO_USUARIO,
    c.NOMBRE_CARGO,
    e.NOMBRE_ESTADO AS ESTADO
FROM FIDE_USUARIOS_TB u
JOIN FIDE_TIPO_USUARIO_TB tu
    ON u.TIPO_USUARIO_ID_FK = tu.TIPO_USUARIO_ID_PK
JOIN FIDE_CARGOS_TB c
    ON tu.CARGOS_ID_CARGO_FK = c.CARGOS_ID_CARGO_PK
JOIN FIDE_ESTADOS_TB e
    ON u.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
ORDER BY u.USUARIO_ID_USUARIO_PK
;
--------------------------------------------------------
--  DDL for View FIDE_VISTA_INVENTARIO_ADMIN_V
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."FIDE_VISTA_INVENTARIO_ADMIN_V" ("ID_MOVIMIENTO", "ID_ESTADO", "ID_ALMACENADO", "ID_PRODUCTO", "NOMBRE_PRODUCTO", "PRECIO_UNITARIO", "ALMACENADO", "CANTIDAD_STOCK", "ESTADO_INVENTARIO", "FECHA_INGRESO", "OBSERVACIONES") AS 
  SELECT
    I.INVENTARIO_ID_MOVIMIENTO_PK AS ID_MOVIMIENTO,
    I.ESTADOS_ID_ESTADO_FK AS ID_ESTADO,
    I.ALMACENADO_ID_ALMACENADO_FK AS ID_ALMACENADO,
    P.PRODUCTO_ID_PRODUCTO_PK AS ID_PRODUCTO,
    P.NOMBRE AS NOMBRE_PRODUCTO,
    P.PRECIO_UNITARIO,
    C.DESCRIPCION_ALMACENADO AS ALMACENADO,
    I.CANTIDAD_STOCK,
    E.NOMBRE_ESTADO AS ESTADO_INVENTARIO,
    I.FECHA_INGRESO,
    I.OBSERVACIONES
FROM FIDE_INVENTARIO_TB I
JOIN FIDE_PRODUCTOS_TB P
    ON I.PRODUCTOS_ID_PRODUCTO_FK = P.PRODUCTO_ID_PRODUCTO_PK
JOIN FIDE_ALMACENADO_TB C
    ON I.ALMACENADO_ID_ALMACENADO_FK = C.ALMACENADO_ID_ALMACENADO_PK
JOIN FIDE_ESTADOS_TB E
    ON I.ESTADOS_ID_ESTADO_FK = E.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View VW_AlMACENAMIENTO_LISTAR
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."VW_AlMACENAMIENTO_LISTAR" ("ALMACENADO_ID_ALMACENADO_PK", "DESCRIPCION_ALMACENADO") AS 
  SELECT ALMACENADO_ID_ALMACENADO_PK, DESCRIPCION_ALMACENADO
FROM FIDE_ALMACENADO_TB
WHERE ESTADOS_ID_ESTADO_FK = 1
;
--------------------------------------------------------
--  DDL for View VW_FACTURACION_LISTAR
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."VW_FACTURACION_LISTAR" ("FACTURACION_ID_FACTURA_PK", "PEDIDOS_ID_PEDIDO_FK", "METODOS_PAGO_ID_PAGO_FK", "METODO_PAGO", "ESTADOS_ID_ESTADO_FK", "ESTADO", "FECHA_EMISION", "DESCUENTOS", "SUBTOTAL", "IVA", "TOTAL_FACTURADO") AS 
  SELECT
    f.FACTURACION_ID_FACTURA_PK,
    f.PEDIDOS_ID_PEDIDO_FK,
    f.METODOS_PAGO_ID_PAGO_FK, -- agregar
    mp.NOMBRE_METODO AS METODO_PAGO,
    f.ESTADOS_ID_ESTADO_FK,    -- agregar
    e.NOMBRE_ESTADO AS ESTADO,
    f.FECHA_EMISION,
    TO_NUMBER(f.DESCUENTOS) AS DESCUENTOS,
    TO_NUMBER(f.SUBTOTAL) AS SUBTOTAL,
    TO_NUMBER(f.IVA) AS IVA,
    TO_NUMBER(f.TOTAL_FACTURADO) AS TOTAL_FACTURADO
FROM FIDE_FACTURACION_TB f
LEFT JOIN FIDE_METODOS_PAGO_TB mp
    ON f.METODOS_PAGO_ID_PAGO_FK = mp.METODOS_PAGO_ID_PAGO_PK
LEFT JOIN FIDE_ESTADOS_TB e
    ON f.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
;
--------------------------------------------------------
--  DDL for View VW_LISTAR_CATEGORIAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "PROYECTOLENGUAJES"."VW_LISTAR_CATEGORIAS" ("CATEGORIA_ID_CATEGORIA_PK", "NOMBRE_CATEGORIA", "DESCRIPCION", "NOMBRE_ESTADO") AS 
  SELECT 
    c.CATEGORIA_ID_CATEGORIA_PK,
    c.NOMBRE_CATEGORIA,
    c.DESCRIPCION,
    e.NOMBRE_ESTADO
FROM FIDE_CATEGORIA_TB c
LEFT JOIN FIDE_ESTADOS_TB e
    ON c.ESTADOS_ID_ESTADO_FK = e.ESTADOS_ID_ESTADO_PK
ORDER BY c.CATEGORIA_ID_CATEGORIA_PK
;
