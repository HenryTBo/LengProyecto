--  DDL for Trigger FIDE_ALMACENADO_ID_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "PROYECTOLENGUAJES"."FIDE_ALMACENADO_ID_TRG" 
BEFORE INSERT ON FIDE_ALMACENADO_TB
FOR EACH ROW
BEGIN
    :NEW.ALMACENADO_ID_ALMACENADO_PK := ALMACENADO_ID_SEQ.NEXTVAL;
END;
/
ALTER TRIGGER "PROYECTOLENGUAJES"."FIDE_ALMACENADO_ID_TRG" ENABLE;
