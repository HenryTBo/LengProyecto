-- FIDE_FACTURACION_TB
-- Inserción
 PROCEDURE FIDE_FACTURACION_INSERTAR_SP (
    P_PEDIDOS_ID_PEDIDO_FK IN NUMBER,
    P_METODOS_PAGO_ID_PAGO_FK IN NUMBER,
    P_FECHA_EMISION IN DATE,
    P_DESCUENTOS IN NUMBER,
    P_SUBTOTAL IN NUMBER,
    P_IVA IN NUMBER,
    P_TOTAL_FACTURADO IN NUMBER,
    P_ESTADOS_ID_ESTADO_FK IN NUMBER
) IS
BEGIN
    INSERT INTO FIDE_FACTURACION_TB (PEDIDOS_ID_PEDIDO_FK, METODOS_PAGO_ID_PAGO_FK,
        FECHA_EMISION, DESCUENTOS, SUBTOTAL, IVA, TOTAL_FACTURADO,
        ESTADOS_ID_ESTADO_FK
    ) VALUES (P_PEDIDOS_ID_PEDIDO_FK, P_METODOS_PAGO_ID_PAGO_FK,
        P_FECHA_EMISION, P_DESCUENTOS, P_SUBTOTAL, P_IVA, P_TOTAL_FACTURADO,
        P_ESTADOS_ID_ESTADO_FK
    );
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

PROCEDURE FIDE_FACTURACION_GENERAR_SP (
    p_pedido_id IN NUMBER,
    p_metodo_pago IN NUMBER,
    p_total_facturado IN NUMBER,
    p_subtotal IN NUMBER,
    p_iva IN NUMBER,
    p_descuentos IN NUMBER
) IS
BEGIN
    INSERT INTO FIDE_FACTURACION_TB (
        TOTAL_FACTURADO,
        SUBTOTAL,
        PEDIDOS_ID_PEDIDO_FK,
        METODOS_PAGO_ID_PAGO_FK,
        IVA,
        DESCUENTOS,
        FECHA_EMISION,
        ESTADOS_ID_ESTADO_FK
   ) VALUES (
    ROUND(p_total_facturado,2),
    ROUND(p_subtotal,2),
    p_pedido_id,
    p_metodo_pago,
    ROUND(p_iva,2),
    ROUND(p_descuentos,2),
    SYSDATE,
    11
);

UPDATE FIDE_PEDIDOS_TB
    SET ESTADOS_ID_ESTADO_FK = 12
    WHERE PEDIDOS_ID_PEDIDO_PK = p_pedido_id;
    COMMIT;
END;


-- Modificación
 PROCEDURE FIDE_FACTURACION_MODIFICAR_SP (
     P_FACTURACION_ID_FACTURA_PK IN NUMBER,
    P_DESCUENTOS IN NUMBER,
    P_SUBTOTAL IN NUMBER,
    P_IVA IN NUMBER,
    P_TOTAL_FACTURADO IN NUMBER,
    P_METODO_PAGO_ID IN NUMBER,
    P_ESTADO_ID IN NUMBER
) IS
BEGIN
    UPDATE FIDE_FACTURACION_TB
    SET DESCUENTOS = ROUND(P_DESCUENTOS, 2),
        SUBTOTAL = ROUND(P_SUBTOTAL, 2),
        IVA = ROUND(P_IVA, 2),
        TOTAL_FACTURADO = ROUND(P_TOTAL_FACTURADO, 2),
        METODOS_PAGO_ID_PAGO_FK = P_METODO_PAGO_ID,
        ESTADOS_ID_ESTADO_FK = P_ESTADO_ID
    WHERE FACTURACION_ID_FACTURA_PK = P_FACTURACION_ID_FACTURA_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

 PROCEDURE GENERAR_FACTURA(
        p_pedido_id   IN NUMBER,
        p_metodo_pago IN NUMBER
    )
    IS
        v_total_pedido NUMBER(10,2);
        v_subtotal     NUMBER(10,2);
        v_iva          NUMBER(10,2);
        v_descuentos   NUMBER(10,2) := 0;
        v_estado_factura NUMBER := 12; -- Estado inicial: Emitida
    BEGIN
        -- Obtener total del pedido
        SELECT TOTAL
        INTO v_total_pedido
        FROM FIDE_PEDIDOS_TB
        WHERE PEDIDOS_ID_PEDIDO_PK = p_pedido_id;

        -- Calcular subtotal e IVA
        v_subtotal := ROUND(v_total_pedido / 1.13, 2);
        v_iva := ROUND(v_total_pedido - v_subtotal, 2);

        -- Insertar la factura
        INSERT INTO FIDE_FACTURACION_TB (
            PEDIDOS_ID_PEDIDO_FK,
            METODOS_PAGO_ID_PAGO_FK,
            ESTADOS_ID_ESTADO_FK,
            SUBTOTAL,
            IVA,
            TOTAL_FACTURADO,
            DESCUENTOS,
            FECHA_EMISION
        )
        VALUES (
            p_pedido_id,
            p_metodo_pago,  -- ahora viene como parámetro
            v_estado_factura,
            v_subtotal,
            v_iva,
            v_total_pedido,
            v_descuentos,
            SYSDATE
        );

        -- Actualizar estado del pedido a 12 (Facturado)
        UPDATE FIDE_PEDIDOS_TB
        SET ESTADOS_ID_ESTADO_FK = 12
        WHERE PEDIDOS_ID_PEDIDO_PK = p_pedido_id;

        COMMIT;

    EXCEPTION
        WHEN NO_DATA_FOUND THEN
            RAISE_APPLICATION_ERROR(-20001, 'No se encontró el pedido con ID ' || p_pedido_id);
        WHEN OTHERS THEN
            ROLLBACK;
            RAISE;
    END GENERAR_FACTURA;

-- Eliminación lógica
 PROCEDURE FIDE_FACTURACION_ELIMINAR_SP (
    P_FACTURACION_ID_FACTURA_PK IN NUMBER
) IS
BEGIN
    UPDATE FIDE_FACTURACION_TB
    SET ESTADOS_ID_ESTADO_FK = 4
    WHERE FACTURACION_ID_FACTURA_PK = P_FACTURACION_ID_FACTURA_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

-- FIDE_INVENTARIO_TB
-- Inserción
PROCEDURE FIDE_INVENTARIO_INSERTAR_SP (
    P_PRODUCTOS_ID_PRODUCTO_FK IN NUMBER,
    P_ESTADOS_ID_ESTADO_FK IN NUMBER,
    P_ALMACENADO_ID_ALMACENADO_FK IN NUMBER,
    P_CANTIDAD_STOCK IN NUMBER,
    P_OBSERVACIONES IN VARCHAR2
) IS
BEGIN
    INSERT INTO FIDE_INVENTARIO_TB (
        PRODUCTOS_ID_PRODUCTO_FK,
        ESTADOS_ID_ESTADO_FK,
        ALMACENADO_ID_ALMACENADO_FK,
        CANTIDAD_STOCK,
        FECHA_INGRESO,
        OBSERVACIONES
    ) VALUES (
        P_PRODUCTOS_ID_PRODUCTO_FK,
        P_ESTADOS_ID_ESTADO_FK,
        P_ALMACENADO_ID_ALMACENADO_FK,
        P_CANTIDAD_STOCK,
        SYSDATE,  -- Fecha automática
        P_OBSERVACIONES
    );

    COMMIT;
EXCEPTION
    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END FIDE_INVENTARIO_INSERTAR_SP;



-- Modificación
PROCEDURE FIDE_INVENTARIO_MODIFICAR_SP (
    P_INVENTARIO_ID_MOVIMIENTO_PK IN NUMBER,
    P_CANTIDAD_STOCK IN NUMBER,
    P_ID_ALMACENADO IN NUMBER,
    P_ID_ESTADO IN NUMBER,
    P_OBSERVACIONES IN VARCHAR2
) IS
BEGIN
    UPDATE FIDE_INVENTARIO_TB
    SET CANTIDAD_STOCK             = P_CANTIDAD_STOCK,
        ALMACENADO_ID_ALMACENADO_FK = P_ID_ALMACENADO,
        ESTADOS_ID_ESTADO_FK        = P_ID_ESTADO,
        OBSERVACIONES               = P_OBSERVACIONES
    WHERE INVENTARIO_ID_MOVIMIENTO_PK = P_INVENTARIO_ID_MOVIMIENTO_PK;
EXCEPTION
    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END FIDE_INVENTARIO_MODIFICAR_SP;



-- Eliminación lógica
 PROCEDURE FIDE_INVENTARIO_ELIMINAR_SP (
    P_INVENTARIO_ID_MOVIMIENTO_PK IN NUMBER
) IS
BEGIN
    UPDATE FIDE_INVENTARIO_TB
    SET ESTADOS_ID_ESTADO_FK = 2
    WHERE INVENTARIO_ID_MOVIMIENTO_PK = P_INVENTARIO_ID_MOVIMIENTO_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

-- FIDE_ALMACENADO_TB
-- Inserción
 PROCEDURE FIDE_ALMACENADO_INSERTAR_SP (
    P_ESTADOS_ID_ESTADO_FK IN NUMBER,
    P_DESCRIPCION_ALMACENADO IN VARCHAR2
) IS
BEGIN
    INSERT INTO FIDE_ALMACENADO_TB (ESTADOS_ID_ESTADO_FK, DESCRIPCION_ALMACENADO
    ) VALUES (
        P_ESTADOS_ID_ESTADO_FK, P_DESCRIPCION_ALMACENADO
    );
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;


-- Modificación
 PROCEDURE FIDE_ALMACENADO_MODIFICAR_SP (
    P_ALMACENADO_ID_ALMACENADO_PK IN NUMBER,
    P_DESCRIPCION_ALMACENADO IN VARCHAR2
) IS
BEGIN
    UPDATE FIDE_ALMACENADO_TB
    SET DESCRIPCION_ALMACENADO = P_DESCRIPCION_ALMACENADO
    WHERE ALMACENADO_ID_ALMACENADO_PK = P_ALMACENADO_ID_ALMACENADO_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;


