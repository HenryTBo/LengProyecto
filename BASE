    VALUES (P_DIRECCION_ID_DIRECCION_PK,P_USUARIOS_ID_USUARIO_FK,P_PAISES_ID_PAIS_FK,P_PROVINCIAS_ID_PROVINCIA_FK,P_CANTONES_ID_CANTON_FK,P_DISTRITOS_ID_DISTRITO_FK,P_OTRAS_SENAS,P_ESTADOS_ID_ESTADO);
    COMMIT;
