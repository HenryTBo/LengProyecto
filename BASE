    COMMIT;
EXCEPTION
    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('ERROR al actualizar email: ' || SQLERRM);
END FIDE_USUARIOS_ACTUALIZAR_EMAIL_SP;

