        ESTADOS_ID_ESTADO_FK
    ) VALUES (
        P_FACTURACION_ID_FACTURA_PK, P_PEDIDOS_ID_PEDIDO_FK, P_METODOS_PAGO_ID_PAGO_FK,
        P_FECHA_EMISION, P_DESCUENTOS, P_SUBTOTAL, P_IVA, P_TOTAL_FACTURADO,
