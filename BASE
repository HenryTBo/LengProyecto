-- FIDE_CARRITO_TB
-- Inserción
 PROCEDURE FIDE_CARRITO_INSERTAR_SP (
    pIdUsuario      IN NUMBER,
    pIdProducto     IN NUMBER
)
AS
    vCantidadActual      NUMBER;
    vCantidadInventario  NUMBER;
    vExiste              NUMBER;
BEGIN
    -- Verificar si el producto ya está en el carrito
    SELECT COUNT(*) INTO vExiste
    FROM FIDE_CARRITO_TB
    WHERE USUARIOS_ID_USUARIO_FK = pIdUsuario
      AND PRODUCTOS_ID_PRODUCTO_FK = pIdProducto
      AND ESTADOS_ID_ESTADO_FK = 1; -- activo

    IF vExiste = 0 THEN
        INSERT INTO FIDE_CARRITO_TB (
            CARRITO_ID_CARRITO_PK,
            USUARIOS_ID_USUARIO_FK,
            PRODUCTOS_ID_PRODUCTO_FK,
            ESTADOS_ID_ESTADO_FK,
            CANTIDAD,
            FECHA_AGREGADO
        ) VALUES (
            FIDE_CARRITO_SEQ.NEXTVAL,
            pIdUsuario,
            pIdProducto,
            1,
            1,
            SYSDATE
        );
    ELSE
        -- Obtener cantidad actual en carrito
        SELECT CANTIDAD INTO vCantidadActual
        FROM FIDE_CARRITO_TB
        WHERE USUARIOS_ID_USUARIO_FK = pIdUsuario
          AND PRODUCTOS_ID_PRODUCTO_FK = pIdProducto
          AND ESTADOS_ID_ESTADO_FK = 1;

        -- Obtener cantidad disponible en inventario
        SELECT CANTIDAD_STOCK INTO vCantidadInventario
        FROM FIDE_INVENTARIO_TB
        WHERE PRODUCTOS_ID_PRODUCTO_FK = pIdProducto
          AND ESTADOS_ID_ESTADO_FK = 1;

        -- Solo actualizar si hay stock disponible
        IF vCantidadActual + 1 <= vCantidadInventario THEN
            UPDATE FIDE_CARRITO_TB
            SET CANTIDAD = CANTIDAD + 1,
                FECHA_AGREGADO = SYSDATE
            WHERE USUARIOS_ID_USUARIO_FK = pIdUsuario
              AND PRODUCTOS_ID_PRODUCTO_FK = pIdProducto
              AND ESTADOS_ID_ESTADO_FK = 1;
        END IF;
    END IF;
END;



-- Modificación
 PROCEDURE FIDE_CARRITO_MODIFICAR_SP (
    P_CARRITO_ID_CARRITO_PK IN NUMBER,
    P_CANTIDAD IN NUMBER
) IS
BEGIN
    UPDATE FIDE_CARRITO_TB
    SET CANTIDAD = P_CANTIDAD
    WHERE CARRITO_ID_CARRITO_PK = P_CARRITO_ID_CARRITO_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;


-- Eliminación lógica
 PROCEDURE FIDE_CARRITO_ELIMINAR_SP (
    P_CARRITO_ID IN NUMBER,
    P_PRODUCTO_ID IN NUMBER
) IS
BEGIN
    UPDATE FIDE_CARRITO_TB
    SET ESTADOS_ID_ESTADO_FK = 2
    WHERE CARRITO_ID_CARRITO_PK = P_CARRITO_ID
      AND PRODUCTOS_ID_PRODUCTO_FK = P_PRODUCTO_ID;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

-- FIDE_PEDIDOS_TB
-- Inserción


PROCEDURE FIDE_PEDIDOS_INSERTAR_LOCAL_SP (
    pIdUsuario IN NUMBER
)
AS
    vTotal    NUMBER(10,2);
    vPedidoId NUMBER;
BEGIN
    -- Inserto el total en la variable
    SELECT SUM(C.CANTIDAD * P.PRECIO_UNITARIO * 1.13)
    INTO vTotal
    FROM FIDE_CARRITO_TB C
    JOIN FIDE_PRODUCTOS_TB P 
        ON C.PRODUCTOS_ID_PRODUCTO_FK = P.PRODUCTO_ID_PRODUCTO_PK
    WHERE C.USUARIOS_ID_USUARIO_FK = pIdUsuario;

    -- Inserto el pedido
    INSERT INTO FIDE_PEDIDOS_TB (
        PEDIDOS_ID_PEDIDO_PK,
        USUARIOS_ID_USUARIO_FK,
        ESTADOS_ID_ESTADO_FK,
        FECHA_PEDIDO,
        TOTAL
    )
    VALUES (
        FIDE_PEDIDOS_SEQ.NEXTVAL,
        pIdUsuario,
        1,
        SYSDATE,
        vTotal
    );

    SELECT MAX(PEDIDOS_ID_PEDIDO_PK)
    INTO vPedidoId
    FROM FIDE_PEDIDOS_TB
    WHERE USUARIOS_ID_USUARIO_FK = pIdUsuario;

    -- Insertar el detalle
    INSERT INTO FIDE_DETALLE_PEDIDO_TB (
        PEDIDOS_ID_PEDIDO_FK,
        PRODUCTOS_ID_PRODUCTO_FK,
        ESTADOS_ID_ESTADO_FK,
        CANTIDAD,
        PRECIO_UNITARIO
    )
    SELECT
        vPedidoId,
        C.PRODUCTOS_ID_PRODUCTO_FK,
        3,
        C.CANTIDAD,
        P.PRECIO_UNITARIO
    FROM FIDE_CARRITO_TB C
    JOIN FIDE_PRODUCTOS_TB P 
        ON C.PRODUCTOS_ID_PRODUCTO_FK = P.PRODUCTO_ID_PRODUCTO_PK
    WHERE C.USUARIOS_ID_USUARIO_FK = pIdUsuario;

    -- Inventario
            UPDATE FIDE_INVENTARIO_TB I
        SET I.CANTIDAD_STOCK = I.CANTIDAD_STOCK - (
            SELECT SUM(C.CANTIDAD)
            FROM FIDE_CARRITO_TB C
            WHERE C.PRODUCTOS_ID_PRODUCTO_FK = I.PRODUCTOS_ID_PRODUCTO_FK
              AND C.USUARIOS_ID_USUARIO_FK = pIdUsuario
        )
        WHERE EXISTS (
            SELECT 1
            FROM FIDE_CARRITO_TB C
            WHERE C.PRODUCTOS_ID_PRODUCTO_FK = I.PRODUCTOS_ID_PRODUCTO_FK
              AND C.USUARIOS_ID_USUARIO_FK = pIdUsuario
        );

    -- Carrito
    DELETE FROM FIDE_CARRITO_TB
    WHERE USUARIOS_ID_USUARIO_FK = pIdUsuario;

    COMMIT;
END;

 PROCEDURE FIDE_PEDIDOS_INSERTAR_SP (
    P_PEDIDOS_ID_PEDIDO_PK IN NUMBER,
    P_USUARIOS_ID_USUARIO_FK IN NUMBER,
    P_ESTADOS_ID_ESTADO_FK IN NUMBER,
    P_FECHA_PEDIDO IN DATE,
    P_TOTAL IN NUMBER
) IS
BEGIN
    INSERT INTO FIDE_PEDIDOS_TB (
        PEDIDOS_ID_PEDIDO_PK, USUARIOS_ID_USUARIO_FK, ESTADOS_ID_ESTADO_FK,
        FECHA_PEDIDO, TOTAL
    ) VALUES (
        P_PEDIDOS_ID_PEDIDO_PK, P_USUARIOS_ID_USUARIO_FK, P_ESTADOS_ID_ESTADO_FK,
        P_FECHA_PEDIDO, P_TOTAL
    );
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

-- Modificación
 PROCEDURE FIDE_PEDIDOS_MODIFICAR_SP (
    P_PEDIDOS_ID_PEDIDO_PK IN NUMBER,
    P_ESTADOS_ID_ESTADO_FK IN NUMBER,
    P_TOTAL IN NUMBER
) IS
BEGIN
    UPDATE FIDE_PEDIDOS_TB
    SET TOTAL = P_TOTAL, ESTADOS_ID_ESTADO_FK =  P_ESTADOS_ID_ESTADO_FK
    WHERE PEDIDOS_ID_PEDIDO_PK = P_PEDIDOS_ID_PEDIDO_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;


-- Eliminación lógica
 PROCEDURE FIDE_PEDIDOS_ELIMINAR_SP (
    P_PEDIDOS_ID_PEDIDO_PK IN NUMBER
) IS
BEGIN
    UPDATE FIDE_PEDIDOS_TB
    SET ESTADOS_ID_ESTADO_FK = 2
    WHERE PEDIDOS_ID_PEDIDO_PK = P_PEDIDOS_ID_PEDIDO_PK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

-- FIDE_DETALLE_PEDIDO_TB
-- Inserción
 PROCEDURE FIDE_DETALLE_PEDIDO_INSERTAR_SP (
    P_PEDIDOS_ID_PEDIDO_FK IN NUMBER,
    P_PRODUCTOS_ID_PRODUCTO_FK IN NUMBER,
    P_ESTADOS_ID_ESTADO_FK IN NUMBER,
    P_CANTIDAD IN NUMBER,
    P_PRECIO_UNITARIO IN NUMBER
) IS
BEGIN
    INSERT INTO FIDE_DETALLE_PEDIDO_TB (PEDIDOS_ID_PEDIDO_FK,
        PRODUCTOS_ID_PRODUCTO_FK, ESTADOS_ID_ESTADO_FK,
        CANTIDAD, PRECIO_UNITARIO
    ) VALUES (P_PEDIDOS_ID_PEDIDO_FK,
        P_PRODUCTOS_ID_PRODUCTO_FK, P_ESTADOS_ID_ESTADO_FK,
        P_CANTIDAD, P_PRECIO_UNITARIO
    );
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;


-- Modificación
 PROCEDURE FIDE_DETALLE_PEDIDO_MODIFICAR_SP (
     P_PEDIDOS_ID_PEDIDO_FK IN NUMBER,
    P_PRODUCTOS_ID_PRODUCTO_FK IN NUMBER,
    P_ESTADOS_ID_ESTADO_FK IN NUMBER
) IS
BEGIN
    UPDATE FIDE_DETALLE_PEDIDO_TB
    SET ESTADOS_ID_ESTADO_FK = P_ESTADOS_ID_ESTADO_FK
    WHERE PEDIDOS_ID_PEDIDO_FK = P_PEDIDOS_ID_PEDIDO_FK
      AND PRODUCTOS_ID_PRODUCTO_FK = P_PRODUCTOS_ID_PRODUCTO_FK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;


-- Eliminación lógica
 PROCEDURE FIDE_DETALLE_PEDIDO_ELIMINAR_SP (
     P_PEDIDOS_ID_PEDIDO_FK IN NUMBER,
    P_PRODUCTOS_ID_PRODUCTO_FK IN NUMBER
) IS
BEGIN
    UPDATE FIDE_DETALLE_PEDIDO_TB
    SET ESTADOS_ID_ESTADO_FK = 2
    WHERE PEDIDOS_ID_PEDIDO_FK = P_PEDIDOS_ID_PEDIDO_FK
      AND PRODUCTOS_ID_PRODUCTO_FK = P_PRODUCTOS_ID_PRODUCTO_FK;
    COMMIT;
EXCEPTION
    WHEN OTHERS THEN DBMS_OUTPUT.PUT_LINE('ERROR: ' || SQLERRM);
END;

